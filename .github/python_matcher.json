{
    "problemMatcher": [
        {
            "owner": "yapf-diff",
            "pattern": [
                {
                    "regexp": "^(?:---|\\+\\+\\+)\\s*([^\\s]*)\\s*\\((?:original|reformatted)\\)$",
                    "file": 1,
                    "message": "This file needs formating."
                }
            ]
        },
        {
            "owner": "pylint",
            "pattern": [
                {
                    "regexp": "^PYLINT:(.*)/.*$",
                    "fromPath": 1
                },
                {
                    "regexp": "^\\*{13}\\s*Module\\s+(.*)$",
                    "file": 1
                },
                {
                    "regexp": "^([CEFIRW]\\d{4}):\\s*(\\d+)\\s*:\\s*(.*)$",
                    "code": 1,
                    "line": 2,
                    "message": 3
                }
            ]
        }
    ]
}
